{
  "name": "zeiss.app.react-templates",
  "version": "0.1.0",
  "private": true,
  "homepage": "/tarot/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.4",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "antd": "^5.0.3",
    "antd-mobile": "^5.29.1",
    "axios": "^1.2.0",
    "classnames": "^2.3.2",
    "crypto-js": "^4.1.1",
    "framer-motion": "^12.16.0",
    "http-proxy-middleware": "^2.0.6",
    "i18next": "^22.4.13",
    "immer": "^10.0.2",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "postcss-scss": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.4.4",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.6",
    "sass": "^1.56.1",
    "sort-by": "^1.2.0",
    "throttle-debounce": "^5.0.0",
    "typescript": "^4.9.3",
    "use-immer": "^0.9.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "dotenv -e .env.local craco start",
    "build": "craco build",
    "build:dev": "dotenv -e .env.dev craco build",
    "build:test": "dotenv -e .env.test craco build",
    "build:prod": "dotenv -e .env.prod craco build",
    "test": "craco test",
    "eject": "craco eject",
    "commit": "npm run pre-commit && cz",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@craco/craco": "^7.1.0",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.191",
    "@types/throttle-debounce": "^5.0.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "2.8.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "src/**/*.+(js|json|ts|tsx)": [
      "eslint --fix"
    ],
    "**/*.{less,css,scss}": "stylelint --fix \"src/**/*.scss\" --custom-syntax postcss-scss"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
